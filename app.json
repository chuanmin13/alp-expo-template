{
  "expo": {
    "name": "ALP EXPO",
    "slug": "alp-expo-template",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "alpexpotemplate",
    "userInterfaceStyle": "automatic",
    "ios": {
      "buildNumber": "0",
      "supportsTablet": true,
      "bundleIdentifier": "com.alp.alp-expo-template",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "CFBundleAllowMixedLocalizations": true,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 57,
      "package": "com.alp.alp-expo-template",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptiveIcon.png",
        "backgroundColor": "#000000"
      },
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "POST_NOTIFICATIONS"
      ]
    },
    "locales": {
      "en-US": "./languages/en-US.json",
      "zh-TW": "./languages/zh-TW.json"
    },
    "plugins": [
      "./plugins/index.js",
      "@react-native-firebase/app",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "android": {
            "resizeMode": "cover"
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "buildReactNativeFromSource": true,
            "deploymentTarget": "15.5",
            "forceStaticLinking": [
              "RNFBApp"
            ]
          }
        }
      ]
    ]
  }
}