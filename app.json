{
  "expo": {
    "name": "ALP EXPO",
    "slug": "alp-expo-template",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "alpexpotemplate",
    "userInterfaceStyle": "automatic",
    "ios": {
      "buildNumber": "0",
      "supportsTablet": true,
      "bundleIdentifier": "com.alp.alp-expo-template",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 57,
      "package": "com.alp.omegaapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptiveIcon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
      ]
    },
    "locales": {
      "en-US": "./languages/en-US.json",
      "zh-TW": "./languages/zh-TW.json"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "android": {
            "resizeMode": "cover"
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ]
  }
}
